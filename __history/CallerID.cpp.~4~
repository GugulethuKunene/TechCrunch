#pragma hdrstop

#include "CallerID.h"

  callerID::callerID()
  {
	Date = Now();
	sTime = "00:00";
	eTime = "12:00";
	lineUsed = 000;
	digits = 0118567564;
	ISP = "Network";
  }

  void callerID::setValues(AnsiString date1,AnsiString time1,AnsiString time2,
   AnsiString lCode,AnsiString tens,AnsiString netW)
  {
	 Date = DateTime(date1);
	sTime = time1;
	eTime = time2;
	lineUsed = lCode;
	digits = tens;
	ISP = netW;
  }
	AnsiString callerID::getNumbers()
	{
		return digits;
	}
	TDate callerID::getDate()
	{
		return Date

	}
	AnsiString callerID::createForm()
	{



	}
